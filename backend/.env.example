# =============================================================================
# ENVIRONMENT VARIABLES EXAMPLE
# =============================================================================
# Copy this file to .env and customize for your environment.
# Do NOT commit .env files with sensitive data to version control.
# =============================================================================

# -----------------------------------------------------------------------------
# Application Settings
# -----------------------------------------------------------------------------
APP_NAME=Digital Footprint Analyzer
DEBUG=False

# -----------------------------------------------------------------------------
# CORS Configuration
# -----------------------------------------------------------------------------
# Comma-separated list of allowed origins
# Add your frontend domain(s) here for production
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# -----------------------------------------------------------------------------
# NLP Configuration
# -----------------------------------------------------------------------------
# spaCy model to use: en_core_web_sm (small), en_core_web_md (medium), en_core_web_lg (large)
SPACY_MODEL=en_core_web_sm
